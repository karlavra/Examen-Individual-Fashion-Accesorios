doctype
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        title Accesorios Fashion
        link(rel="stylesheet" type="text/css" href="css/bootstrap.min.css")
        link(rel="stylesheet" type="text/css" href="css/font-awesome.min.css") 
        link(rel="stylesheet" type="text/css" href="sass/main.css")
    body
        include header
        section
            div.container
                div.col-md-6
                    iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3104.9007294838657!2d-77.02656301203302!3d38.90338536116687!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89b7b793f5477f0f%3A0xe7df0df629523829!2sNew+York+Ave%2C+Washington%2C+DC+20005%2C+EE.+UU.!5e0!3m2!1ses-419!2spe!4v1459354969742" width="600" height="450" frameborder="0" style="border:0" allowfullscreen)
                div.col-md-6
                    form.form-inline
                        div.form-group
                            input.form-control(type="name" placeholder="Firts name")
                            input.form-control(type="name" placeholder="Last name")
                        div.form-group
                            input.form-control(type="email" placeholder="Email")
                            input.form-control(type="site" placeholder="Website")
                    form
                        input.form-control(type="text"  placeholder="Subject")
                        textarea.form-control(rows="3" placeholder="Message")
                        
        include footer                  
        script(src="js/bootstrap.min.js")
        script(src="js/jquery.js")      